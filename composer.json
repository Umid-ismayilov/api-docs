{
    "name": "br-tech/api-docs",
    "description": "API documentation package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Umid Ismayilov",
            "email": "umid.ismayilov01@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "br-tech/api-docs": "dev-main",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Br\\ApiDocsPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Br\\ApiDocsPackage\\ApiDocsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
